    <tags>
        <tag>Clay</tag>
        <tag>Soft clay</tag>
        <tag>Humidify</tag>
    </tags>

    <resources>
        <resource>/com/TheVTM/bots/ClayWetanabe/log.dev.properties</resource>
        <resource>/com/TheVTM/bots/ClayWetanabe/log.properties</resource>
